{{log this}}
<div class=card1>
<br>
      <button type="generate-lock" class="btn btn-success" id="generate">Generate</button>
      <div id="generatedItem"></div>
      </div>
      
{{#each listData as |List| }}
<br>
      <div class="card2">
      <div id="BucketList">
        <h2>{{List.title}}</h2>
        <section class="category-div">
        <h3>Travel</h3>
        {{#each List.bucketListItems as |item| }}
        {{#if (ifCond item.category "travel")}}
        <ul id="travel-list">{{item.body}}</ul>
        {{/if}}
        {{/each}}
        </section>
      
        <section class="category-div">
        <h3>Food</h3>
        {{#each List.bucketListItems as |item| }}
        {{#if (ifCond item.category "food")}}
        <ul id="food-list">{{item.body}}</ul>
        {{/if}}
        {{/each}}
        </section>
      
    
       <section>
        <h3>Adrenaline</h3>
        {{#each List.bucketListItems as |item| }}
        {{#if (ifCond item.category "adrenaline")}}
        <ul id="adrenaline-list">{{item.body}}</ul>
        {{/if}}
        {{/each}}
        </section>
        </div>
        </div>
        <br>
          {{/each}}
          <div class="containerL">
        <div class="card3">
          <a href="/login">
          <button type="register" class="btn btn-primary">Login</button>
          </a>
          <a href="/profile">
          <button type="register" class="btn btn-primary">View Profile</button>
          </a>
        </div>
        </section>
</div>
  
     
