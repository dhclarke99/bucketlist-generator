{{log this}}
<div class="container">
  <div class="card7">
    <div id="BucketList">
      <h4>{{title}}</h4>
      <div class="container2">
        <button type="travel-lock" class="btn btn-primary" id="travel">Travel</button>
        <br>
        {{#each bucketListItems as |item| }}
        {{#if (ifCond item.category "travel")}}
        <ul id="travel-list">{{item.body}}
          <button id ="{{item.id}}">Delete Item</button>
        </ul>
        
        {{/if}}
        {{/each}}


        <br>

        <button type="travel-lock" class="btn btn-primary" id="food">Food</button>
        <br>
        {{#each bucketListItems as |item| }}
        {{#if (ifCond item.category "food")}}
        <ul id="food-list">{{item.body}}
          <button>Delete Item</button>
        </ul>
        
        {{/if}}
        {{/each}}
        <br>
        <button type="travel-lock" class="btn btn-primary" id="adrenaline">Adrenaline</button>
        <br>
        {{#each bucketListItems as |item| }}
        {{#if (ifCond item.category "adrenaline")}}
        <ul id="adrenaline-list">{{item.body}}
          <button>Delete Item</button>
        </ul>
        
        {{/if}}
        {{/each}}

        <br>


      </div>
    </div>
  </div>
</div>
<br>
<div class="container">
 <div class = "card7">
    <input class="user-input" placeholder="Write your own idea here!">
  </div>
  </div>
<div class="container">
  <div class="card7">
    <button type="submit" class="btn btn-danger" id="clear">Clear List</button>
  </div>
</div>



<h3>Share your Bucket List:</h3>
